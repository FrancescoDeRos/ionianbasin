accessori/convolvi.csh
convolvi.csh
../../sintetici/doevt.2012.sh
/home/common/sintetici/fixcoord.run
#plot
ls *HHZd.sac | wc -l 
lh kstnm stla stlo stel stdp evla evlo evel evdp delta

beginframe
ticks off all
qdp off
xvport 0.08 0.48
picks off
color off
xlim off
ylim off
fileid type list kstnm kcmpnm 
fileid location ul
r *HHZd.sac 
sort dist
dc 6-17
bp c 0.02 0.3 n 3 p 1
xlim 0 30
mul 1000000
plabel 1 "Observed signals EQ20221012224455.0 Mw 4.3 " position 0.15 0.9
xlabel "Time [s]"
ylabel "Ground displacement [micron]"
p1
plabel 1 off

ls *Z.semdc.sac | wc -l
xvport 0.56 0.96
color 1
r *Z.semdc.sac
sort dist
dc 6-17
bp
mul 1000000
plabel 1 "Synthetic signals triple layer EQ20221012224455.0 Mw 4.3 " position 0.65 0.9
xlabel "Time [s]"
ylabel "Ground displacement [micron]"
p1 
endframe
saveimg 3strat-bccatlidovs20221012_topogra.pdf
